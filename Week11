package week11;

public class Employee {
	//attributes
	private String name;
	private int age ;
	private int emplId;
	private double hourlyPay;
	private String role;
	
	//Constructor
	public Employee() {
		this.name = name;
		this.age = age;
		this.emplId = emplId;
		this.hourlyPay = hourlyPay;
		this.role = role;
	}
		
		public Employee(String name, int age, int emplId, double hourlyPay, String role) {
			this.name = "jane";
			this.age = 20;
			this.emplId = 345678;
			this.hourlyPay = 15.0;
			this.role = "tutorial";
			
		}
		
		public Employee (String name, String role) {
			this.name = "Layla";
			this.role = "Sleeping";
		}
		public String get (String name) {
			return this.name;
		}
			
		public int get () {
			return this.age;
		}
		
		public int get (int emplId) {
			return this.emplId;
			
		}
		
		public double get (double hourlyPay) {
			return this.hourlyPay;
		}
		
		public String get1 (String role) {
			return this.role;
		}
		
		
		public void set (String name) {
			this.name = name;
		}
		public void set1 (int age) {
			this.age = age;
			
			
	}
		public void set (int emplId) {
			this.emplId = emplId;
		}
		 
		public void set (double hourlyPay) {
			this.hourlyPay = hourlyPay;
		}
		
		public void set1 (String role) {
			this.role = role;
		}
		
		public boolean equals (Object Dobject) {
			if(this == Dobject) {
				return true;
			}
			if(Dobject is InstanceOF (Employee) {
				Employee c1 = (Employee) Dobject;
				if (this.name == c1.age)
					
			}
			return false;
			
		}
	
	

}
